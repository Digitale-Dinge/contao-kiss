{
    "name": "digitaledinge/contao-kiss",
    "description": "Digitale Dinge's super simple starterkit for Contao CMS",
    "keywords": [
        "contao",
        "kiss",
        "contao kiss",
        "base framework"
    ],
    "type": "contao-bundle",
    "homepage": "https://github.com/Digitale-Dinge/contao-kiss",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Digitale Dinge GmbH",
            "homepage": "https://digitaledin.ge",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hallo@digitaledin.ge",
        "issues": "https://github.com/Digitale-Dinge/contao-kiss/issues",
        "source": "https://github.com/Digitale-Dinge/contao-kiss"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DigitaleDinge\\ContaoKiss\\": "src/"
        },
        "exclude-from-classmap": [
            "contao/dca/",
            "contao/languages/",
            "contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "DigitaleDinge\\ContaoKiss\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": false,
            "contao/manager-plugin": true
        }
    }
}
